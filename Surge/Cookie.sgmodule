#!name= Cookie
#!desc= 该模块适用百度极速版,中青极速版,抖音极速版获取Cookie
#!system=ios

[Script]
百度极速版Cookie = type=http-request,pattern=https:\/\/haokan\.baidu\.com\/activity\/h5\/vault\?productid=\d,script-path=https://raw.githubusercontent.com/yangshuima90/GithubSync/main/BaiDu/baidu.js

中青极速版签到Cookie = type=http-request https:\/\/kd\.youth\.cn\/WebApi\/NewTaskIos\/getTaskList,script-path=https://raw.githubusercontent.com/yangshuima90/GithubSync/main/YouTh/youth.js

抖音极速版Cookie.01 = type=http-request,pattern=luckycat/aweme/v1/task/done/treasure_task?,script-path=https://raw.githubusercontent.com/yangshuima90/GithubSync/main/DouYin/douyin.js

抖音极速版Cookie.02 = type=http-request,pattern=luckycat/aweme/v1/task/done/treasure_task?,script-path=https://raw.githubusercontent.com/yangshuima90/GithubSync/main/DouYin/douyin.js

抖音极速版Cookie.03 = type=http-request,pattern=luckycat/aweme/v1/task/done/excitation_ad_treasure_box?,script-path=https://raw.githubusercontent.com/yangshuima90/GithubSync/main/DouYin/douyin.js

抖音极速版Cookie.04 = type=http-request,pattern=uckycat/aweme/v1/task/done/excitation_ad?,script-path=https://raw.githubusercontent.com/yangshuima90/GithubSync/main/DouYin/douyin.js

抖音极速版Cookie.05 = type=http-request,pattern=luckycat/aweme/v1/task/done/read?,script-path=https://raw.githubusercontent.com/yangshuima90/GithubSync/main/DouYin/douyin.js

抖音极速版Cookie.06 = type=http-request,pattern=luckycat/aweme/v1/task/walk/step_submit?,script-path=https://raw.githubusercontent.com/yangshuima90/GithubSync/main/DouYin/douyin.js

[MITM]
hostname = haokan.baidu.com,ios.baertt.com, kd.youth.cn, kandian.wkandian.com,*.amemv.com
